name: 增强流媒体检测
desc: 支持 Netflix/Disney+/ChatGPT/YouTube/TikTok
author: xiaomingyve1
update-url: https://github.com/xiaomingyve1/Stash
enable: true
tiles:
  - name: liumeitijiance
    icon: "play.tv.fill"
    label-color: "#FF2D55"
    background-color: "#FF2D55"
script-providers:
  liumeitijiance:
    url: https://raw.githubusercontent.com/xiaomingyve1/Stash/main/Script/media-detection.js
    no-cache: false
    schedule:
      - cron: "* * * * *"
      - interval: 300
      - network-change
      - manual
    policy: no-cache
